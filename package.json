		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.16.0",
		"svelte-check": "^4.1.1",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.2",
		"vite": "^5.4.11"
	}
